			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>en_US</supported-locale>
		<supported-locale>de_DE</supported-locale>
		<resource-bundle>de.uhh.l2g.resources.Resource</resource-bundle>
		<portlet-info>
			<title>Metadata</title>
			<short-title>Metadata</short-title>
			<keywords>Producer, MetaData</keywords>
		</portlet-info>
	</portlet>
 
